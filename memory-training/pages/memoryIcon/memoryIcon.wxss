/**index.wxss**/
.menory-icon {
  width: 100%;
  height: 100%;
  text-align: center;
}
.beforeStart{
  width: 100%;
  height: 100%;
  background-color:rgb(34,35,37);
  padding-top: 50px;
}
.beforeStart .start{
  font-size: 20px;
  padding: 20px;
  color: #fff;
}
.beforeStart .start-hover{
  color: rgb(196, 196, 44);
}
.beforeStart .tips{
  font-size: 14px;
  color: rgb(228, 222, 222);
}

.started{

}
.started .icon-list .list-item {
  font-size: 20px;
  width: 100rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  border: 1px solid #eee;
}

.nopass-page {
  width: 100%;
  height: 100%;
  position: relative;
  /* padding-top: 100rpx; */
  /* background-image: url('../../assets/image/gogogo2.jpg'); */
  /* background-size: 100% auto; */
}
.nopass-page .tips{
  padding-top: 150rpx;
}
.nopass-page .tips1{
  color:red;
  background-color:rgba(0,0,0,0.5);
  font-size: 24px;
  padding: 25rpx;
}
.nopass-page .tips2{
  color:red;
  background-color:rgba(0,0,0,0.5);
  font-size: 16px;
  padding: 25rpx;
}
.nopass-page .bgimg {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}
.nopass-page .bottom-btn {
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 50rpx;
  display: flex;
  justify-content: space-around;
}


